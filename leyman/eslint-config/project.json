{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "@leyman/eslint-config",
  "targets": {
    "biome": {},
    "eslint": {},
    "npm-update-ts-references": {},
    "delete-dist": {},
    "populate-static": {},
    "tsc": {},
    "coverage-reset": {},
    "mocha-unit-test": {},
    "mocha-integration-test": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "c8 --clean=false --reporter=none eslint . './**/*.{c,m,}ts{x,}' './**/*.{c,m,}js'"
        ],
        "parallel": true,
        "color": true,
        "cwd": "{projectRoot}"
      },
      "dependsOn": [
        "mocha-unit-test",
        "test:reset"
      ],
      "cache": true,
      "inputs": [
        "ts-source",
        "^ts-output"
      ],
      "outputs": [
        "{projectRoot}/coverage/tmp"
      ]
    },
    "coverage-report": {},
    "analyze:_": {},
    "analyze:format": {},
    "analyze:lint": {},
    "analyze": {},
    "build:_": {},
    "build:pre": {},
    "build:run": {},
    "build:post": {},
    "build": {},
    "install:_": {},
    "install": {},
    "test:_": {},
    "test:reset": {},
    "test:run": {},
    "test:report": {},
    "test": {}
  }
}
