{
  "name": "barrelify",
  "version": "1.1.8",
  "description": "Auto-generate TS barrel files.",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "author": "jacobley",
  "license": "MIT",
  "repository": "jacobley/jacobley",
  "homepage": "https://github.com/JacobLey/jacobley/tree/main/apps/barrelify#readme",
  "bugs": {
    "url": "https://github.com/jacobley/jacobley/issues"
  },
  "keywords": [
    "barrel",
    "index"
  ],
  "sideEffects": false,
  "types": "./dist/barrelify.js",
  "exports": {
    ".": "./dist/barrelify.js"
  },
  "bin": {
    "barrel-local": "./cli.mjs",
    "barrelify-local": "./cli.mjs"
  },
  "publishConfig": {
    "bin": {
      "barrel": "./dist/cli.js",
      "barrelify": "./dist/cli.js"
    }
  },
  "dependencies": {
    "default-import": "workspace:^",
    "entry-script": "workspace:^",
    "find-import": "workspace:^",
    "globby": "^13.2.2",
    "named-patch": "workspace:^",
    "parse-cwd": "workspace:^",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.8.10",
    "@types/sinon": "^17.0.3",
    "@types/yargs": "^17.0.29",
    "c8": "^8.0.1",
    "chai": "^5.1.0",
    "expect-type": "^0.17.3",
    "mocha": "^10.3.0",
    "mocha-hookup": "workspace:^",
    "nx-tsc": "workspace:^",
    "nx-update-ts-references": "workspace:^",
    "sinon": "^17.0.1",
    "ts-dedent": "^2.2.0",
    "typescript": "^5.3.3"
  }
}
