{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "barrelify",
  "targets": {
    "biome": {},
    "eslint": {},
    "update-ts-references": {},
    "tsc": {},
    "coverage-reset": {},
    "mocha-unit-test": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "c8 --clean=false --reporter=none mocha --node-option conditions=patchable --recursive './dist/tests/unit/**/*.spec.*js'"
        ],
        "parallel": true,
        "color": true,
        "cwd": "{projectRoot}"
      },
      "dependsOn": ["test:reset"]
    },
    "coverage-report": {},
    "analyze:_": {},
    "analyze:format": {},
    "analyze:lint": {},
    "analyze": {},
    "build:_": {},
    "build:pre": {},
    "build:run": {},
    "build:post": {},
    "build": {},
    "test:_": {},
    "test:reset": {},
    "test:run": {},
    "test:report": {},
    "test": {}
  }
}
