{
  "name": "haywire",
  "version": "0.1.7",
  "description": "Type safe dependency injection",
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "author": "JacobLey",
  "license": "MIT",
  "repository": "JacobLey/leyman",
  "homepage": "https://github.com/JacobLey/leyman/tree/main/tools/haywire#readme",
  "bugs": {
    "url": "https://github.com/JacobLey/leyman/issues"
  },
  "keywords": [
    "typescript",
    "dependency injection",
    "inversion of control",
    "type-safe"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js"
  },
  "imports": {
    "#binding": "./dist/lib/binding.js",
    "#container": "./dist/lib/container.js",
    "#errors": "./dist/lib/errors.js",
    "#factory": "./dist/lib/factory.js",
    "#identifier": "./dist/lib/identifier.js",
    "#module": "./dist/lib/module.js",
    "#scopes": "./dist/lib/scopes.js",
    "#types": "./dist/lib/types.js"
  },
  "dependencies": {
    "p-defer": "catalog:"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@leyman/eslint-config": "workspace:^",
    "@swc/cli": "catalog:",
    "@swc/core": "catalog:",
    "@types/chai": "catalog:",
    "@types/chai-as-promised": "catalog:",
    "@types/mocha": "catalog:",
    "@types/node": "catalog:",
    "c8": "catalog:",
    "chai": "catalog:",
    "chai-as-promised": "catalog:",
    "eslint": "catalog:",
    "expect-type": "catalog:",
    "mocha": "catalog:",
    "npm-barrelify": "catalog:leyman",
    "npm-nx-update-ts-references": "catalog:leyman",
    "npm-pnpm-dedicated-lockfile": "catalog:leyman",
    "typescript": "catalog:"
  }
}
