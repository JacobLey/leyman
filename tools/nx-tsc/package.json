    "@biomejs/biome": "^1.6.4",
    "@leyman/eslint-config": "workspace:^",
    "@swc/core": "^1.4.13",
    "@types/chai": "^4.3.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.26",
    "chai": "^5.1.0",
    "mocha": "^10.4.0",
    "npm-load-populate-files": "npm:load-populate-files@^0.0.2",
    "npm-nx-populate-files": "npm:nx-populate-files@^0.0.3",
    "npm-nx-update-ts-references": "npm:nx-update-ts-references@^0.0.2",
    "npm-nx-tsc": "npm:nx-tsc@^0.0.5",
    "nyc": "^15.1.0",
    "typescript": "^5.4.3"
  }
}
