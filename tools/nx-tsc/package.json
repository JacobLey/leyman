{
  "name": "nx-tsc",
  "version": "0.0.5",
  "description": "Build typescript package as nx executor",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "author": "JacobLey",
  "license": "MIT",
  "repository": "JacobLey/leyman",
  "homepage": "https://github.com/JacobLey/leyman/tree/main/tools/nx-tsc#readme",
  "bugs": {
    "url": "https://github.com/JacobLey/leyman/issues"
  },
  "keywords": ["nx", "tsc", "typescript", "build"],
