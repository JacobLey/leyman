{
  "name": "entry-script",
  "version": "3.0.1",
  "description": "Modular control for entry script execution.",
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "author": "JacobLey",
  "license": "MIT",
  "repository": "JacobLey/leyman",
  "homepage": "https://github.com/JacobLey/leyman/tree/main/tools/entry-script#readme",
  "bugs": {
    "url": "https://github.com/JacobLey/leyman/issues"
  },
  "keywords": [
    "executable",
    "entry point",
    "require main"
  ],
  "sideEffects": false,
  "types": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "imports": {
    "#entry-script": "./dist/lib/entry-script.js",
    "#not-implemented-error": "./dist/lib/not-implemented-error.js",
    "#process-argv": "./dist/lib/process-argv.js"
  },
  "dependencies": {
    "default-import": "workspace:^"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@leyman/eslint-config": "workspace:^",
    "@types/chai": "^4.3.17",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.7",
    "@types/node": "^22.1.0",
    "c8": "^10.1.2",
    "chai": "^5.1.1",
    "chai-as-promised": "^8.0.0",
    "mocha": "^10.7.0",
    "mocha-chain": "workspace:^",
    "nx-tsc": "workspace:^",
    "nx-update-ts-references": "workspace:^",
    "typescript": "^5.5.4"
  }
}
