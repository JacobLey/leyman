{
  "name": "entry-script",
  "version": "2.1.1",
  "description": "Modular control for entry script execution.",
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "author": "JacobLey",
  "license": "MIT",
  "repository": "JacobLey/leyman",
  "homepage": "https://github.com/JacobLey/leyman/tree/main/tools/entry-script#readme",
  "bugs": {
    "url": "https://github.com/JacobLey/leyman/issues"
