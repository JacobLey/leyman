    "@types/node": "^20.8.10",
    "@types/sinon": "^17.0.3",
    "c8": "^8.0.1",
    "chai": "^5.1.0",
    "mocha": "^10.3.0",
    "mocha-hookup": "workspace:^",
    "nx-tsc": "workspace:^",
    "nx-update-ts-references": "workspace:^",
    "sinon": "^17.0.1",
    "typescript": "^5.3.3"
