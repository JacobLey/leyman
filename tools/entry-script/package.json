    "@types/node": "^20.14.10",
    "@types/sinon": "^17.0.3",
    "c8": "^10.1.2",
    "chai": "^5.1.1",
    "mocha": "^10.6.0",
    "mocha-hookup": "workspace:^",
    "nx-tsc": "workspace:^",
    "nx-update-ts-references": "workspace:^",
    "sinon": "^18.0.0",
    "typescript": "^5.5.3"
