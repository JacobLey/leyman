{
  "name": "entry-script",
  "version": "3.0.9",
  "description": "Modular control for entry script execution.",
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "author": "JacobLey",
  "license": "MIT",
  "repository": "JacobLey/leyman",
  "homepage": "https://github.com/JacobLey/leyman/tree/main/tools/entry-script#readme",
  "bugs": {
    "url": "https://github.com/JacobLey/leyman/issues"
  },
  "keywords": [
    "executable",
    "entry point",
    "require main"
  ],
  "sideEffects": false,
  "types": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "imports": {
    "#entry-script": "./dist/lib/entry-script.js",
    "#not-implemented-error": "./dist/lib/not-implemented-error.js",
    "#process-argv": "./dist/lib/process-argv.js"
  },
  "dependencies": {
    "default-import": "workspace:^"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@leyman/eslint-config": "workspace:^",
    "@swc/cli": "catalog:",
    "@swc/core": "catalog:",
    "@types/chai": "catalog:",
    "@types/chai-as-promised": "catalog:",
    "@types/mocha": "catalog:",
    "@types/node": "catalog:",
    "c8": "catalog:",
    "chai": "catalog:",
    "chai-as-promised": "catalog:",
    "eslint": "catalog:",
    "mocha": "catalog:",
    "mocha-chain": "workspace:^",
    "npm-nx-update-ts-references": "catalog:leyman",
    "npm-pnpm-dedicated-lockfile": "catalog:leyman",
    "typescript": "catalog:"
  }
}
